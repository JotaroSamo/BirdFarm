@model BirdFarm.ModelsBD.Egg
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/css/Home.css" rel="stylesheet" />
    <link href="~/css/Auth.css" rel="stylesheet" />
</head>
<body class="back">
    <header class="header_up">
        <a class="logout-button" asp-area="" asp-controller="Home" asp-action="Logout">Log out</a>
        <a asp-area="" asp-controller="Admin" asp-action="Tools">Tools</a>
    </header>
    <form asp-controller="Admin" asp-action="AddEgg" method="post">
        <div>
            <label>Egg Type:</label>
            <input type="text" name="EggType" />
        </div>
        <div>
            <label>Egg Color:</label>
            <input type="text" name="EggColor"/>
        </div>
        <div>
            <label>Count Eggs:</label>
            <input type="text" name="CountEggs" />
        </div>
        <div>
            <label>Price:</label>
            <input type="text" name="Price" />
        </div>
        <input type="submit" value="Save">
    </form>

</body>
</html>